"editor.tokenColorCustomizations": {
    "comments": "#7b7b7b",
    // "strings": "#0099CC",
    // "keywords": "#00FF00",
    "textMateRules": [
        {
            "scope": [
                "source.dart entity.name.class",
                "source.dart support.class",
                "source.dart constant.other",
                "source.dart variable.other.readwrite",
                "source.dart variable.other",
            ],
            "settings": {
                "foreground": "#24b59d",
                "fontStyle": "bold italic"
            }
        },
        {
            "scope": [
                "variable.other.constant.tsx",
            ],
            "settings": {
                "foreground": "#bababa" // Change this to your desired color
            }
        },
        {
            "scope": [
                // "variable.other.constant.tsx",
                "variable.other.property.tsx",
                "variable.other.object.property.tsx",
            ],
            "settings": {
                "foreground": "#9a9a9a" // Change this to your desired color
            }
        },
        {
            "name": "Color-1",
            "scope": [
                "constant.other.color",
                "keyword.other.substitution",
                "keyword.other.template",
                "punctuation.section.embedded",
                "punctuation.separator.inheritance.php",
                "punctuation",
                "source"
            ],
            "settings": {
                // "foreground": "#ECF6FF"
            }
        },
        {
            "name": "Color-1 bold",
            "scope": [
                "support.type.object",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "support.type.property-name",
                "variable.graphql",
                "variable.parameter",
                "entity.name.type.class",
                "entity.name.type.interface",
                "entity.name.type.module",
                "support.class.builtin",
                "variable.object.property",
                "meta.object-literal",
                "support.class",
                "entity.name.function.tagged-template"
            ],
            "settings": {
                "fontStyle": "bold",
                // "foreground": "#ECF6FF"
            }
        },
        {
            "name": "Color-1 italic",
            "scope": [
                "markup.italic",
                "meta.property-value",
                "source.css constant",
                "source.css support.constant",
                "source.stylus constant",
                "source.stylus support.constant",
                "string",
                "constant.numeric.css",
                "string.quoted.single",
                "string.template",
                "support.function.console",
                "meta.function-call.object"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#5eb3b8"
            }
        },
        {
            "name": "Color-1 underline",
            "scope": [
                "entity.name.function",
            ],
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#ecdc61"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Color-1 italic bold",
            "scope": [
                "support.variable.object",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "keyword.other.unit",
                "keyword.other",
                "entity.name.type.class.php",
                "keyword.function.go",
            ],
            "settings": {
                "fontStyle": "italic bold",
                // "foreground": "#ECF6FF"
            }
        },
        {
            "name": "Color-1 italic underline bold",
            "scope": ["variable.function.constructor", "meta.function-call.static"],
            "settings": {
                "fontStyle": "italic underline bold",
                // "foreground": "#ECF6FF"
            }
        },
        {
            "name": "Color-1 italic underline",
            "scope": ["variable.function", "markup.underline.link"],
            "settings": {
                "fontStyle": "italic underline",
                // "foreground": "#ECF6FF"
            }
        },
    
        {
            "name": "Color-2",
            "scope": [
                "constant.numeric.decimal",
                "constant.other.reference.link.markdown",
                "entity.other.attribute-name.class",
                "entity.other.inherited-class",
                "punctuation.section.embedded",
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "source.stylus string",
                "entity.name.type.instance",
                "meta.type.object.graphql",
                "punctuation.definition.tag",
                "entity.name.fragment.graphql",
                "meta.function.php",
                "variable.other.php",
            ],
            "settings": {
                "fontStyle": "",
                // "foreground": "#EBEA8B"
            }
        },
        {
            "name": "Color-2 italic",
            "scope": ["variable.parameter.graphql","support.function.magic","storage.type.shape"],
            "settings": {
                "fontStyle": "italic",
                // "foreground": "#EBEA8B"
            }
        },
        {
            "name": "Color-2 bold",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
            ],
            "settings": {
                "fontStyle": "bold",
                // "foreground": "#EBEA8B"
            }
        },
        {
            "name": "Color-3",
            "scope": [
                "meta.property.object",
                "string.other.link.title.markdown",
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "support.type",
                "string.regexp",
                "variable.other",
                "variable.other.bracket.shell",
                "variable.other.readwrite",
                "variable.other.property",
            ],
            "settings": {
                "fontStyle": "",
                // "foreground": "#AEE9F5"
            }
        },
        {
            "name": "Color-3 italic",
            "scope": [
                "variable.other.object",
                "variable.other.readwrite.alias",
                "variable.fragment.graphql",
                "entity.name.type.typedecl"
            ],
            "settings": {
                "fontStyle": "italic",
                // "foreground": "#AEE9F5"
            }
        },
        {
            "name": "Color-3 bold",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "support.class.component",
                "entity.name.tag"
            ],
            "settings": {
                "fontStyle": "bold",
                // "foreground": "#AEE9F5"
            }
        },
        {
            "name": "Color-3 italic bold",
            "scope": [
                "entity.name.tag",
                "markup.deleted.git_gutter",
                "support.class.component"
            ],
            "settings": {
                "fontStyle": "italic bold",
                // "foreground": "#AEE9F5"
            }
        },
        {
            "name": "Color-3 italic underline",
            "scope": [],
            "settings": {
                "fontStyle": "italic underline",
                // "foreground": "#AEE9F5"
            }
        },
        {
            "name": "Color-4",
            "scope": [
                "support.type",
                "variable.other.constant",
                "support.variable.property",
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "string.other.link",
                "meta.property-list.scss",
                "markup.inserted",
                "meta.type.object.graphql",
                "support.type.primitive",
        
        
            ],
            "settings": {
                "fontStyle": "",
                // "foreground": "#A8F79A"
            }
            },
            {
            "name": "Color-4 italic",
            "scope": [
                "constant.character.escape",
                "entity.name.type",
                "entity.other.attribute-name",
                "support.class.console",
                "support.function",
                "support.type.graphql",
                "storage.type.php"
            ],
            "settings": {
                "fontStyle": "italic",
                // "foreground": "#A8F79A"
            }
        },
        {
            "name": "Color-4 italic underline",
            "scope": [
        
            ],
            "settings": {
                "fontStyle": "italic underline",
                // "foreground": "#A8F79A"
            }
        },
        {
            "name": "Color-4 underline",
            "scope": ["markup.underline"],
            "settings": {
                "fontStyle": "underline",
                // "foreground": "#A8F79A"
            }
        },
        {
            "name": "Color-4 bold",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json",
                "markup.underline",
                "entity.name.tag.css"
            ],
            "settings": {
                "fontStyle": "bold",
                // "foreground": "#A8F79A"
            }
        },
        {
            "name": "Color-5",
            "scope": [
                "keyword",
                "punctuation",
                "punctuation.operation.graphql",
                "storage.modifier.php",
                "storage.type",
            ],
            "settings": {
                "fontStyle": "",
                // "foreground": "#F699D9"
            }
        },
        {
            "scope": [
                "constant.language",
                "storage.modifier",
                "entity.name.tag.grapahql.js",
                "constant.other"
            ],
            "settings": {
                "fontStyle": "bold",
                // "foreground": "#F699D9"
            }
        },
        {
            "name": "Color-5 italic",
            "scope": [
                "keyword.control",
                "punctuation.definition",
                "keyword.fragment.graphql"
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#b165a9"
            }
        },
        {
            "name": "Color-5 bold",
            "scope": ["keyword.generator.asterisk"],
            "settings": {
                "fontStyle": "italic bold",
                // "foreground": "#F699D9"
            }
        },
        {
            "name": "Color-6",
            "scope": "comment",
            "settings": {
                // "foreground": "#A3B5C3"
                "fontStyle": "italic"
            }
        },
        {
            "name": "Color-6 italic",
            "scope": [
                "punctuation.definition.comment",
            ],
            "settings": {
                "fontStyle": "italic",
                // "foreground": "#A3B5C3"
            }
        }
    ],
    "function.declaration.readonly": {
        "foreground": "#ECF6FF",
        "fontStyle": "underline"
    },
    "property.declaration": {
        "foreground": "#ECF6FF",
        "fontStyle": "bold"
    },
    "function.readonly": {
        "foreground": "#ECF6FF",
        "fontStyle": "italic underline"
    },
    "parameter.declaration": {
        "foreground": "#ECF6FF",
        "fontStyle": ""
    },
    "function.readonly.local": {
        "foreground": "#ECF6FF",
        "fontStyle": "italic underline"
    },
    "variable.declaration.readonly": {
        "foreground": "#EBEA8B",
        "fontStyle": "italic"
    },
    "variable.readonly": {
        "foreground": "#EBEA8B",
        "fontStyle": ""
    },
    "*.readonly": {
        "foreground": "#EBEA8B",
        "fontStyle": ""
    },
    "property": {
        "foreground": "#AEE9F5",
        "fontStyle": ""
    },
    "parameter": {
        "foreground": "#AEE9F5",
        "fontStyle": ""
    },
    "type": {
        "foreground": "#A8F79A",
        "fontStyle": "italic"
    },
    "type.declaration": {
        "foreground": "#A8F79A",
        "fontStyle": "italic"
    },
    "interface.defaultLibrary": {
        "foreground": "#A8F79A",
        "fontStyle": "italic"
    },
    "variable.defaultLibrary": {
        "foreground": "#A8F79A",
        "fontStyle": "bold"
    },
    "member.defaultLibrary": {
        "foreground": "#AEE9F5",
        "fontStyle": "italic"
    },
    "*.defaultLibrary": {
        "foreground": "#AEE9F5",
        "fontStyle": "italic"
    }
},
